type Reservation {
    id: ID!
    date: String!
    startTime: String!
    endTime: String!
    user: User!
    workspace: Workspace!
}

input ReservationInput {
    date: String!
    startTime: String!
    endTime: String!
    userId: ID!
    workspaceId: ID!
}

extend type Query {
    getReservations: [Reservation!]!
    getReservationById(id: ID!): Reservation
}

extend type Mutation {
    createReservation(input: ReservationInput!): Reservation!
    cancelReservation(id: ID!): Boolean!
}

